{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-euclid",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "no-empty": true
  },
  "rules": {
    "no-angle-bracket-type-assertion": true,
    "no-unsafe-any": true,
    "no-any": {
      "severity": "warning"
    },
    "no-submodule-imports": [
      true,
      "rxjs"
    ]
  }
}
